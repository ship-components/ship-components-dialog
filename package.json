{
  "name": "ship-components-dialog",
  "version": "1.0.10",
  "description": "React material design modals",
  "main": "src/index.js",
  "scripts": {
    "start": "grunt server",
    "build": "npm run test && grunt build",
    "test": "grunt lint && ./node_modules/jest-cli/bin/jest.js --coverage",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ship-components/ship-components-dialog.git"
  },
  "contributors": [
    {
      "name": "Isaac Suttell",
      "email": "isaac.suttell@sony.com"
    },
    {
      "name": "Chris Orescan",
      "email": "chris.orescan@sony.com"
    },
    {
      "name": "Jared Boone",
      "email": "jared.boone@sony.com"
    },
    {
      "name": "Sepand Assadi",
      "email": "sepand.assadi@sony.com"
    }
  ],
  "keywords": [
    "react",
    "material design",
    "dialog",
    "alert",
    "confirm"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ship-components/ship-components-dialog/issues"
  },
  "homepage": "https://github.com/ship-components/ship-components-dialog#readme",
  "dependencies": {
    "react": ">=15.0.0",
    "react-dom": ">=15.0.0",
    "ship-components-buttons": "^1.0.2",
    "ship-components-outsideclick": "^0.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^18.0.0",
    "babel-loader": "^9.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.11.1",
    "bluebird": ">=3",
    "classnames": "^2.2.5",
    "coveralls": "^2.11.12",
    "css-loader": "^0.26.1",
    "eslint": "^3.3.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.1.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-eslint": "^19.0.0",
    "grunt-webpack": "^1.0.14",
    "invariant": "^2.2.1",
    "jest-cli": "^18.1.0",
    "jest-css-modules": "^1.1.0",
    "postcss-calc": "^6.0.2",
    "postcss-color-function": "^2.0.1",
    "postcss-color-hex-alpha": "^2.0.0",
    "postcss-loader": "^1.2.1",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react-transition-group": "^1.2.1",
    "style-loader": "^0.13.1",
    "webpack": "^5.74.0",
    "webpack-dev-server": "^1.15.0"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/classnames",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/bluebird"
    ],
    "setupFiles": [
      "raf/polyfill"
    ]
  },
  "babelJest": {
    "include": [
      "src/**/*.@(js|jsx|es6)",
      "node_modules/ship-components-highlight-click/src/**/*.@(js|jsx|es6)"
    ]
  }
}
